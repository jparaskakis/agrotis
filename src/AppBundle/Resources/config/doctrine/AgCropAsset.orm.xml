<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AgCropAsset" table="ag_crop_asset">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="producer" type="integer" column="producer" nullable="true"/>
    <field name="crop" type="integer" column="crop" nullable="true"/>
    <field name="url" type="string" column="url" length="245" nullable="true"/>
    <field name="size" type="float" column="size" precision="10" scale="0" nullable="true"/>
  </entity>
</doctrine-mapping>
